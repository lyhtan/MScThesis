@article{rayyan-958928438,
  title={Diseases diagnosis using fuzzy logic methods: A systematic and meta-analysis review},
  year={2018},
  journal={Computer Methods and Programs in Biomedicine},
  issn={01692607 (ISSN)},
  volume={161},
  pages={145-172},
  author={Ahmadi, H. and Gholamzadeh, M. and Shahmoradi, L. and Nilashi, M. and Rashvand, P.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85046134112&doi=10.1016%2fj.cmpb.2018.04.013&partnerID=40&md5=eed336b26a5b0134f667c8b7260521da},
  language={English},
  keywords={algorithm, Algorithms, Clinical decision making, clinical decision support system, clinical feature, Computer circuits, data base, Decision making, decision support system, Decision Support Systems, Clinical, diagnosis, Diagnosis, diagnostic imaging, Diagnostic Imaging, Disease diagnosis, diseases, fuzzy decision tree, fuzzy logic, Fuzzy logic, Fuzzy Logic, Fuzzy methods, fuzzy neural network, fuzzy system, human, Humans, image processing, Image Processing, Computer-Assisted, Inclusion and exclusions, machine learning, Machine Learning, Medical disciplines, medical literature, medical practice, Medical problems, meta analysis, Models, Theoretical, PRISMA, procedures, Review, Scientific database, software, Software, systematic review, theoretical model, uncertainty, Uncertainty},
  abstract={Background and Objective: Diagnosis as the initial step of medical practice, is one of the most important parts of complicated clinical decision making which is usually accompanied with the degree of ambiguity and uncertainty. Since uncertainty is the inseparable nature of medicine, fuzzy logic methods have been used as one of the best methods to decrease this ambiguity. Recently, several kinds of literature have been published related to fuzzy logic methods in a wide range of medical aspects in terms of diagnosis. However, in this context there are a few review articles that have been published which belong to almost ten years ago. Hence, we conducted a systematic review to determine the contribution of utilizing fuzzy logic methods in disease diagnosis in different medical practices. Methods: Eight scientific databases are selected as an appropriate database and Preferred Reporting Items for Systematic Reviews and Meta-Analyses (PRISMA) method was employed as the basis method for conducting this systematic and meta-analysis review. Regarding the main objective of this research, some inclusion and exclusion criteria were considered to limit our investigation. To achieve a structured meta-analysis, all eligible articles were classified based on authors, publication year, journals or conferences, applied fuzzy methods, main objectives of the research, problems and research gaps, tools utilized to model the fuzzy system, medical disciplines, sample sizes, the inputs and outputs of the system, findings, results and finally the impact of applied fuzzy methods to improve diagnosis. Then, we analyzed the results obtained from these classifications to indicate the effect of fuzzy methods in decreasing the complexity of diagnosis. Results: Consequently, the result of this study approved the effectiveness of applying different fuzzy methods in diseases diagnosis process, presenting new insights for researchers about what kind of diseases which have been more focused. This will help to determine the diagnostic aspects of medical disciplines that are being neglected. Conclusions: Overall, this systematic review provides an appropriate platform for further research by identifying the research needs in the domain of disease diagnosis. © 2018 Elsevier B.V.},
  note={<p>Export Date: 15 March 2023; Cited By: 93; Correspondence Address: M. Gholamzadeh; Health Information Management Department, School of Allied Medical Sciences, Tehran University of Medical Sciences, Tehran, No #17, 5th Floor, Farredanesh Alley, Ghods St, Enghelab Ave, Iran; email: m-gholamzadeh@razi.tums.ac.ir; CODEN: CMPBE</p>},
  doi={10.1016/j.cmpb.2018.04.013}
}

@article{rayyan-958928440,
  title={The Role of Social Media and Internet Search Engines in Information Provision and Dissemination to Patients with Kidney Stone Disease: A Systematic Review from European Association of Urologists Young Academic Urologists},
  year={2018},
  journal={Journal of Endourology},
  issn={08927790 (ISSN)},
  volume={32},
  number={8},
  pages={673-684},
  author={Jamnadass, E. and Aboumarzouk, O. and Kallidonis, P. and Emiliani, E. and Tailly, T. and Hruby, S. and Sanguedolce, F. and Atis, G. and Ozsoy, M. and Greco, F. and Somani, B.K.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85051533649&doi=10.1089%2fend.2018.0319&partnerID=40&md5=c1bfd105a57626de2d38f8c5cb63b527},
  language={English},
  keywords={access to information, Access to Information, clinical practice, diet, Europe, fluid, human, Humans, information dissemination, information processing, Internet, Kidney Calculi, kidney stone disease, medical information system, medical society, mobile application, Mobile Applications, nephrolithiasis, patient attitude, patient compliance, Patient Compliance, patient counseling, patient education, Patient Education as Topic, practice guideline, preventive medicine, priority journal, procedures, Professional-Patient Relations, professional-patient relationship, Review, search engine, Search Engine, smartphone, Smartphone, social media, Social Media, Societies, Medical, systematic review, urolithiasis, urologist, Urologists, urology, Urology},
  abstract={Introduction: Kidney stone disease (KSD) affects millions of people worldwide and has an increasing incidence. Social media (SoMe) and search engines are both gaining in usage, while also being used by patients to research their conditions and aid in managing them. With this in mind, many authors have expressed the belief that SoMe and search engines can be used by patients and healthcare professionals to improve treatment compliance and help counseling and management of conditions such as KSD. We wanted to determine whether SoMe and search engines play a role in the management and/or prevention of KSD. Materials and Methods: The databases MEDLINE, Embase, CINAHL, Scopus, and Cochrane Library were used to search for relevant English language literature from inception to December 2017. Results were screened by title, abstract, and then full text, according to the inclusion and exclusion criteria. The data were then analyzed independently by the authors not involved in the original study. Results: After initial identification of 2137 records and screening of 42 articles, 10 studies met the inclusion and exclusion criteria. The articles included focused on a variety of SoMe forms, including two articles each on twitter, YouTube, smartphone apps, and Google search engine and one article on Google insights and Google analytics. Regarding patient centered advice, while two articles covered advice on dietary, fluid intake, and management options, two additional articles each covered advice on fluid advice and management options only, while no such advice was given by three of the SoMe published articles. Conclusions: SoMe and search engines provide valuable information to patients with KSD. However, while the information provided regarding dietary aspects and fluid management was good, it was not comprehensive enough to include advice on other aspects of KSD prevention. © Copyright 2018, Mary Ann Liebert, Inc. 2018.},
  note={<p>Export Date: 15 March 2023; Cited By: 12; Correspondence Address: B.K. Somani; University Hospital Southampton, NHS Trust, Department of Urology, Southampton, SO16 6YD, United Kingdom; email: bhaskarsomani@yahoo.com; CODEN: JENDE</p>},
  doi={10.1089/end.2018.0319}
}

@article{rayyan-958928448,
