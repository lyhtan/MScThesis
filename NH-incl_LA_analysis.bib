}

@article{rayyan-958928660,
  title={Applied computer technologies in clinical decision support systems for pain management: A systematic review},
  year={2014},
  journal={Journal of Intelligent and Fuzzy Systems},
  issn={10641246 (ISSN)},
  volume={26},
  number={5},
  pages={2411-2425},
  author={Pombo, N. and Araújo, P. and Viana, J.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84901751405&doi=10.3233%2fIFS-912&partnerID=40&md5=520008e8596bef29e7f436bf81229f6d},
  language={English},
  keywords={Clinical decision support system, Clinical decision support systems, Clinical practices, Computer technology, Decision support systems, Digital libraries, Electronic database, Fuzzy sets, Health, Learning algorithms, Learning systems, machine learning, medical informatics, Medical informatics, Mobile devices, Neural networks, pain measurement, Rule based algorithms, Statistical learning, Surveys, Web-based interface, Decision Support Systems, Clinical, Pain Management},
  abstract={Millions of people around the world suffer from pain, acute or chronic and this raises the importance of its screening, assessment and treatment. Pain, is highly subjective and the use of clinical decision support systems (CDSSs) can play an important part in improving the accuracy of pain assessment, and lead to better clinical practices. This review examines CDSSs, in relation to computer technologies and was conducted with the following electronic databases: CiteSeerx, IEEE Xplore, ISI Web of Knowledge, Mendeley, Microsoft Academic Search, PubMed, Science Accelerator, Science.gov, ScienceDirect, SpringerLink, and The Cochrane Library. The studies referenced were compiled with several criteria in mind. Firstly, that they constituted a decision support system. Secondly, that study data included pain values or results based on the detection of pain. Thirdly, that they were published in English, between 1992 and 2011, and finally that they focused on patients with acute or chronic pain. In total, thirty-nine studies highlighted the following topics: rule based algorithms, artificial neural networks, rough and fuzzy sets, statistical learning algorithms, terminologies, questionnaires and scores. The median accuracy ranged from 53% to 87.5%. The lack of integration with mobile devices, the limited use of web-based interfaces and the scarcity of systems that allow for data to be inserted by patients were all limitations that were detected. © 2014 - IOS Press and the authors.},
  note={<p>Export Date: 15 March 2023; Cited By: 2; Correspondence Address: N. Pombo; Instituto de Telecomunicações, Department of Informatics, University of Beira Interior, Covilhã, Portugal; email: ngpombo@ubi.pt</p>},
  doi={10.3233/IFS-912}
}

@article{rayyan-958928661,
  title={Decision support and automation bias: Methodology and preliminary results of a systematic review},
  year={2011},
  journal={Studies in Health Technology and Informatics},
  issn={09269630 (ISSN); 978-160750708-6 (ISBN)},
  volume={164},
  pages={3-7},
  author={Goddard, K. and Roudsari, A. and Wyatt, J.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-79953059585&doi=10.3233%2f978-1-60750-709-3-3&partnerID=40&md5=2c0ee2f2716a8e4b0db9911c1cf97c0b},
  language={English},
  publisher={IOS Press},
  keywords={Artificial intelligence, attitude to computers, Attitude to Computers, automation, Automation, Automation bias, Clinical decision making, Clinical decision support systems, Cognition, Decision making, decision support system, Decision support systems, Decision Support Systems, Clinical, Decision supports, health care personnel, Health Personnel, human, Human judgment, Human judgments, Humans, psychology, Reliance, Systematic review, Systematic Review, utilization, Bias (Epidemiology)},
  abstract={Automation bias - or a tendency to over-rely on automation - is a subject which has been studied in a variety of academic fields. Clinical Decision Support Systems (CDSS) aim to benefit the clinical decision making process. Although most research shows overall improved performance with use, there is often a failure to recognize the new errors that CDSS can introduce, and as such the healthcare field has a gap in this research. This paper summarizes the methodology and preliminary results of a systematic review over a broad range of fields into the effects of over-reliance on automation. Results indicate that though automation bias is a significant phenomenon, it is not well defined, and there is a gap in the research which must be addressed to optimize the use of decision support. © 2011 ITCH 2011 Steering Committee and IOS Press.},
  note={<p>Export Date: 15 March 2023; Cited By: 0; Correspondence Address: K. Goddard; Centre for Health Informatics, City University, London, EC1V 0HB, Northampton Square, United Kingdom; email: kate.goddard.1@city.ac.uk</p>},
  doi={10.3233/978-1-60750-709-3-3}
}

