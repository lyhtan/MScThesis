  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85130771181&doi=10.1016%2fj.jacc.2022.03.380&partnerID=40&md5=8e5e7efd76e1453d346b943a12adedf7},
  language={English},
  keywords={acetylsalicylic acid, acute chest pain, acute coronary syndrome, acute heart failure, ambulance, artificial intelligence, Artificial Intelligence, biological marker, Biomarkers, cardiovascular magnetic resonance, Chest Pain, chronic obstructive lung disease, clinical outcomes, computer assisted tomography, congestive heart failure, coronary angiography, coronary artery disease, coronavirus disease 2019, dyspnea, echocardiography, electrocardiogram, electrocardiography, Electrocardiography, emergency health service, Emergency Service, Hospital, fever, follow up, health care, health systems, heart arrhythmia, heart catheterization, heart rate, histopathology, hospital emergency service, human, Humans, lung embolism, machine learning, major adverse cardiac event, models of care, mortality rate, myopericarditis, myosin binding protein C, pandemic, patient satisfaction, percutaneous coronary intervention, personalized medicine, pneumonia, point of care testing, prevalence, pulse oximetry, Review, shared decision making, ST segment elevation myocardial infarction, stress echocardiography, support vector machine, systematic review, thorax pain, troponin, Troponin, vaccination, virus myocarditis, Review Literature as Topic},
  abstract={Existing assessment pathways for acute chest pain are often resource-intensive, prolonged, and expensive. In this review, the authors describe existing chest pain pathways and current issues at the patient and system level, and provide an overview of recent advances in chest pain research that could inform improved outcomes for both patients and health systems. There are multiple avenues to improve existing models of chest pain care, including novel risk stratification pathways incorporating highly sensitive point-of-care troponin assays; new devices available before first medical contact that could allow clinicians to access vital signs and electrocardiogram data; artificial intelligence and precision medicine tools that may guide indications for further testing; and strategies to improve hospital benchmarking and performance monitoring to standardize care. Improving the speed and accuracy of chest pain diagnosis and management should be a priority for researchers and is likely to translate to substantive benefits for patients and health systems. © 2022 American College of Cardiology Foundation},
  note={<p>Export Date: 15 March 2023; Cited By: 2; Correspondence Address: D. Stub; Department of Cardiology, Alfred Health, Prahran, 55 Commercial Road, 3004, Australia; email: d.stub@alfred.org.au; CODEN: JACCD</p>},
  doi={10.1016/j.jacc.2022.03.380}
}

@article{rayyan-958926926,
  title={Artificial intelligence for falls management in older adult care: A scoping review of nurses' role},
  year={2022},
  journal={Journal of Nursing Management},
  issn={09660429 (ISSN)},
  volume={30},
  number={8},
  pages={3787-3801},
  author={O'Connor, S. and Gasteiger, N. and Stanmore, E. and Wong, D.C. and Lee, J.J.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85139925993&doi=10.1111%2fjonm.13853&partnerID=40&md5=ddfbe491b875c48a4d30530cd7002b2d},
  language={English},
  keywords={aged, Aged, algorithm, artificial intelligence, Artificial Intelligence, Cinahl, clinical decision making, Cochrane Library, Embase, falls, female, geriatric nursing, Geriatric Nursing, health care delivery, human, Humans, machine learning, male, Medline, natural language processing, nurse, nurse attitude, Nurse's Role, nursing, nursing management, patient care, predictive model, review, risk factor, systematic review, Intelligence},
  abstract={Aim: This study aims to synthesize evidence on nurses' involvement in artificial intelligence research for managing falls in older adults. Background: Artificial intelligence techniques are used to analyse health datasets to aid clinical decision making, patient care and service delivery but nurses' involvement in this area of research for managing falls in older adults remains unknown. Evaluation: A scoping review was conducted. CINAHL, the Cochrane Library, Embase, MEDLI and PubMed were searched. Results were screened against inclusion criteria. Relevant data were extracted, and studies summarized using a descriptive approach. Key Issues: The evidence shows many artificial intelligence techniques, particularly machine learning, are used to identify falls risk factors and build predictive models that could help prevent falls in older adults, with nurses leading and participating in this research. Conclusion: Further rigorous experimental research is needed to determine the effectiveness of algorithms in predicting aspects of falls in older adults and how to implement artificial intelligence tools in gerontological nursing practice. Implications for Nursing Management: Nurses should pursue interdisciplinary collaborations and educational opportunities in artificial intelligence, so they can actively contribute to research on falls management. Nurses should facilitate the collection of digital falls datasets to support this emerging research agenda and the care of older adults. © 2022 The Authors. Journal of Nursing Management published by John Wiley & Sons Ltd.},
  note={<p>Export Date: 15 March 2023; Cited By: 0; Correspondence Address: S. O'Connor; Division of Nursing, Midwifery and Social Work, School of Health Sciences, The University of Manchester, Manchester, Jean MacFarlane Building, M13 9PL, United Kingdom; email: siobhan.oconnor@manchester.ac.uk; CODEN: JNMNE</p>},
  doi={10.1111/jonm.13853}
}

@article{rayyan-958926928,
  title={Functional neuroimaging in psychiatry and the case for failing better},
  year={2022},
  journal={Neuron},
  issn={08966273 (ISSN)},
  volume={110},
  number={16},
  pages={2524-2544},
  author={Nour, M.M. and Liu, Y. and Dolan, R.J.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85135791320&doi=10.1016%2fj.neuron.2022.07.005&partnerID=40&md5=3f2bc6ef39a634e4466492a121112f35},
  language={English},
  keywords={amino acid sequence, artificial intelligence, BOLD signal, brain, Brain, brain blood flow, brain depth stimulation, brain function, clinical decision making, cognition, cognitive neuroscience, computational psychiatry, decision making, default mode network, depression, diagnostic imaging, EEG, electroencephalogram, electroencephalography, Electroencephalography, electrophysiology, fMRI, functional connectivity, functional magnetic resonance imaging, functional neuroimaging, Functional Neuroimaging, hippocampus, human, Humans, machine learning, Magnetic Resonance Imaging, magnetoencephalography, Magnetoencephalography, MEG, memory, memory consolidation, mental disease, multivariate analysis, nerve cell network, neuroimaging, Neuroimaging, neurophysiology, neuroscience, nuclear magnetic resonance imaging, positron emission tomography, precision psychiatry, procedures, protein structure, psychiatry, Psychiatry, psychosis, psychotherapy, repetitive transcranial magnetic stimulation, Review, schizophrenia, scientific literature, systematic review, transcranial magnetic stimulation},
  abstract={Psychiatric disorders encompass complex aberrations of cognition and affect and are among the most debilitating and poorly understood of any medical condition. Current treatments rely primarily on interventions that target brain function (drugs) or learning processes (psychotherapy). A mechanistic understanding of how these interventions mediate their therapeutic effects remains elusive. From the early 1990s, non-invasive functional neuroimaging, coupled with parallel developments in the cognitive neurosciences, seemed to signal a new era of neurobiologically grounded diagnosis and treatment in psychiatry. Yet, despite three decades of intense neuroimaging research, we still lack a neurobiological account for any psychiatric condition. Likewise, functional neuroimaging plays no role in clinical decision making. Here, we offer a critical commentary on this impasse and suggest how the field might fare better and deliver impactful neurobiological insights. © 2022 Elsevier Inc.},
  note={<p>Export Date: 15 March 2023; Cited By: 7; Correspondence Address: M.M. Nour; Max Planck University College London Centre for Computational Psychiatry and Ageing Research, London, WC1B 5EH, United Kingdom; email: matthew.nour.18@alumni.ucl.ac.uk; R.J. Dolan; Max Planck University College London Centre for Computational Psychiatry and Ageing Research, London, WC1B 5EH, United Kingdom; email: r.dolan@ucl.ac.uk; CODEN: NERNE</p>},
  doi={10.1016/j.neuron.2022.07.005}
}

