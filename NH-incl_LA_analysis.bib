@article{rayyan-958928676,
  title={Integrating language models into classifiers for BCI communication: A review},
  year={2016},
  journal={Journal of Neural Engineering},
  issn={17412560 (ISSN)},
  volume={13},
  number={3},
  author={Speier, W. and Arnold, C. and Pouratian, N.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84973470242&doi=10.1088%2f1741-2560%2f13%2f3%2f031002&partnerID=40&md5=14c59434c6ce30902433b437388ca240},
  language={English},
  keywords={algorithm, Algorithms, amyotrophic lateral sclerosis, Amyotrophic lateral sclerosis, Augmentative and assistive communication devices, Bayesian learning, biometry, brain computer interface, Brain computer interface, Brain-Computer Interfaces, braincomputer interface, classification, Classification (of information), classifier, Classifier performance, clinical evaluation, communication aid, Communication Aids for Disabled, Computational linguistics, data base, decision making, dynamics, electroencephalography, Electroencephalography, Error correction, extraction, hidden Markov model, human, Humans, information processing, integration, Integration, Interfaces (computer), language, Language, language model, Language model, linguistics, machine learning, measurement error, Medline, Models, Theoretical, natural language processing, Natural Language Processing, NAtural language processing, Natural language processing systems, P300 speller, predictive spelling, priority journal, probability, process model, Review, Signal classification, signal processing, speech discrimination, Speech recognition, Speech transmission, systematic review, theoretical model, User interfaces, visual evoked potential},
  abstract={Objective. The present review systematically examines the integration of language models to improve classifier performance in brain-computer interface (BCI) communication systems. Approach. The domain of natural language has been studied extensively in linguistics and has been used in the natural language processing field in applications including information extraction, machine translation, and speech recognition. While these methods have been used for years in traditional augmentative and assistive communication devices, information about the output domain has largely been ignored in BCI communication systems. Over the last few years, BCI communication systems have started to leverage this information through the inclusion of language models. Main results. Although this movement began only recently, studies have already shown the potential of language integration in BCI communication and it has become a growing field in BCI research. BCI communication systems using language models in their classifiers have progressed down several parallel paths, including: word completion; signal classification; integration of process models; dynamic stopping; unsupervised learning; error correction; and evaluation. Significance. Each of these methods have shown significant progress, but have largely been addressed separately. Combining these methods could use the full potential of language model, yielding further performance improvements. This integration should be a priority as the field works to create a BCI system that meets the needs of the amyotrophic lateral sclerosis population. © 2016 IOP Publishing Ltd.},
  note={<p>Export Date: 15 March 2023; Cited By: 31; Correspondence Address: N. Pouratian; Department of Neurosurgery, University of California, Los Angeles, 90095, United States; email: npouratian@mednet.ucla.edu</p>},
  doi={10.1088/1741-2560/13/3/031002}
}

@article{rayyan-958928678,
  title={Systems cell biology knowledge created from high content screening},
  year={2005},
  journal={Assay and Drug Development Technologies},
  issn={1540658X (ISSN)},
  volume={3},
  number={5},
  pages={501-514},
  author={Giuliano, K.A. and Cheung, W.S. and Curran, D.P. and Day, B.W. and Kassick, A.J. and Lazo, J.S. and Nelson, S.G. and Shin, Y. and Taylor, D.L.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-29244462317&doi=10.1089%2fadt.2005.3.501&partnerID=40&md5=959f1b89204ea22ac40a8e65517f89a6},
  language={English},
  keywords={antineoplastic agent, Antineoplastic Agents, article, Artificial Intelligence, automation, bioinformatics, Biological Assay, cell composition, Cell Culture Techniques, cell cycle, cell killing, controlled study, cytology, data base, decision making, discodermolide, Dose-Response Relationship, Drug, Drug Evaluation, Preclinical, drug targeting, high throughput screening, human, human cell, Humans, immunofluorescence, laulimalide, MEDLINE, methodology, microtubule, phenotype, process optimization, Robotics, systematic review, Systems Biology, tumor cell line, Tumor Cells, Cultured},
  abstract={High content screening (HCS), the large-scale automated analysis of the temporal and spatial changes in cells and cell constituents in arrays of cells, has the potential to create enormous systems cell biology knowledge bases. HCS is being employed along with the continuum of the early drug discovery process, including lead optimization where new knowledge is being used to facilitate the decision-making process. We demonstrate methodology to build new systems cell biology knowledge using a multiplexed HCS assay, designed with the aid of knowledge-mining tools, to measure the phenotypic response of a panel of human tumor cell types to a panel of natural product-derived microtubule-targeted anticancer agents and their synthetic analogs. We show how this new systems cell biology knowledge can be used to design a lead compound optimization strategy for at least two members of the panel, (-)-laulimalide and (+)-discodermolide, that exploits cell killing activity while minimally perturbing the regulation of the cell cycle and the stability of microtubules. Furthermore, this methodology can also be applied to basic biomedical research on cells. © Mary Ann Liebert, Inc.},
  note={<p>Export Date: 15 March 2023; Cited By: 41; Correspondence Address: K.A. Giuliano; Cellumen, Inc., Pittsburgh, PA 15203, 2516 Jane Street, United States; email: kgiuliano@cellumen.com; CODEN: ADDTA</p>},
  doi={10.1089/adt.2005.3.501}
}

@article{rayyan-958928684,
  title={Clinical Prediction Models for Cardiovascular Disease: Tufts Predictive Analytics and Comparative Effectiveness Clinical Prediction Model Database},
  year={2015},
  journal={Circulation: Cardiovascular Quality and Outcomes},
  issn={19417713 (ISSN)},
  volume={8},
  number={4},
  pages={368-375},
  author={Wessler, B.S. and Yh, L.L. and Kramer, W. and Cangelosi, M. and Raman, G. and Lutz, J.S. and Kent, D.M.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84937701283&doi=10.1161%2fCIRCOUTCOMES.115.001693&partnerID=40&md5=8c83e3199d27f3e84c02b3295cd64667},
  language={English},
  keywords={acute coronary syndrome, aorta disease, Article, calibration, cardiovascular disease, cardiovascular diseases, Cardiovascular Diseases, cardiovascular risk, cerebrovascular accident, cerebrovascular disorders, coronary artery disease, data base, diabetes mellitus, follow up, Global Health, health, heart arrhythmia, heart failure, human, Humans, individualized medicine, ischemic heart disease, logistic models, Models, Theoretical, morbidity, Morbidity, mortality, outcome assessment, peripheral vascular disease, prediction, priority journal, procedures, prognosis, randomized controlled trial (topic), risk assessment, Risk Assessment, risk factor, Risk Factors, smoking, systematic review, theoretical model, trends, venous thromboembolism, Models, Cardiovascular},
  abstract={Background - Clinical prediction models (CPMs) estimate the probability of clinical outcomes and hold the potential to improve decision making and individualize care. For patients with cardiovascular disease, there are numerous CPMs available although the extent of this literature is not well described. Methods and Results - We conducted a systematic review for articles containing CPMs for cardiovascular disease published between January 1990 and May 2012. Cardiovascular disease includes coronary heart disease, heart failure, arrhythmias, stroke, venous thromboembolism, and peripheral vascular disease. We created a novel database and characterized CPMs based on the stage of development, population under study, performance, covariates, and predicted outcomes. There are 796 models included in this database. The number of CPMs published each year is increasing steadily over time. Seven hundred seventeen (90%) are de novo CPMs, 21 (3%) are CPM recalibrations, and 58 (7%) are CPM adaptations. This database contains CPMs for 31 index conditions, including 215 CPMs for patients with coronary artery disease, 168 CPMs for population samples, and 79 models for patients with heart failure. There are 77 distinct index/outcome pairings. Of the de novo models in this database, 450 (63%) report a c-statistic and 259 (36%) report some information on calibration. Conclusions - There is an abundance of CPMs available for a wide assortment of cardiovascular disease conditions, with substantial redundancy in the literature. The comparative performance of these models, the consistency of effects and risk estimates across models and the actual and potential clinical impact of this body of literature is poorly understood. © 2015 American Heart Association, Inc.},
  note={<p>Export Date: 15 March 2023; Cited By: 59; Correspondence Address: D.M. Kent; Predictive Analytics and Comparative Effectiveness (PACE) Center, Institute for Clinical Research and Health Policy Studies (ICRHPS), Tufts Medical Center/Tufts University School of Medicine, Boston, 800 Washington St, 02111, United States; email: dkent1@tuftsmedicalcenter.org</p>},
  doi={10.1161/CIRCOUTCOMES.115.001693}
}

@article{rayyan-958928685,
  title={A systematic review of predictive modeling for bronchiolitis},
  year={2014},
  journal={International Journal of Medical Informatics},
  issn={13865056 (ISSN)},
  volume={83},
  number={10},
  pages={691-714},
  author={Luo, G. and Nkoy, F.L. and Gesteland, P.H. and Glasgow, T.S. and Stone, B.L.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84906784708&doi=10.1016%2fj.ijmedinf.2014.07.005&partnerID=40&md5=ae812ab63dc1e85a3f0d84af235378f6},
  language={English},
  keywords={accuracy, Artificial intelligence, artificial ventilation, assisted ventilation, bronchiolitis, Bronchiolitis, disease course, hospital admission, Hospitals, human, Humans, Iterative methods, Iterative process, Learning systems, Literature search, Machine learning, Management decisions, Models, Theoretical, oxygen therapy, palivizumab, pathophysiology, Practice variations, Predictive modeling, predictive value, primary medical care, priority journal, Respiratory syncytial pneumovirus, Respiratory syncytial virus, review, systematic review, Systematic Review, theoretical model, thorax radiography, viral bronchiolitis, Viruses},
  abstract={Purpose: Bronchiolitis is the most common cause of illness leading to hospitalization in young children. At present, many bronchiolitis management decisions are made subjectively, leading to significant practice variation among hospitals and physicians caring for children with bronchiolitis. To standardize care for bronchiolitis, researchers have proposed various models to predict the disease course to help determine a proper management plan. This paper reviews the existing state of the art of predictive modeling for bronchiolitis. Predictive modeling for respiratory syncytial virus (RSV) infection is covered whenever appropriate, as RSV accounts for about 70% of bronchiolitis cases. Methods: A systematic review was conducted through a PubMed search up to April 25, 2014. The literature on predictive modeling for bronchiolitis was retrieved using a comprehensive search query, which was developed through an iterative process. Search results were limited to human subjects, the English language, and children (birth to 18 years). Results: The literature search returned 2312 references in total. After manual review, 168 of these references were determined to be relevant and are discussed in this paper. We identify several limitations and open problems in predictive modeling for bronchiolitis, and provide some preliminary thoughts on how to address them, with the hope to stimulate future research in this domain. Conclusions: Many problems remain open in predictive modeling for bronchiolitis. Future studies will need to address them to achieve optimal predictive models. © 2014 Elsevier Ireland Ltd.},
  note={<p>Export Date: 15 March 2023; Cited By: 26; Correspondence Address: G. Luo; Department of Biomedical Informatics, University of Utah, Salt Lake City, UT 84108, Suite 140, 421 Wakara Way, United States; email: gang.luo@utah.edu; CODEN: IJMIF</p>},
  doi={10.1016/j.ijmedinf.2014.07.005}
}

