  issn={14796678 (ISSN)},
  volume={18},
  number={2},
  pages={154-164},
  author={Sotirakos, S. and Fouda, B. and Mohamed Razif, N.A. and Cribben, N. and Mulhall, C. and O'Byrne, A. and Moran, B. and Connolly, R.},
