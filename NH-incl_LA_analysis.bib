  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85126796585&doi=10.1007%2fs12609-022-00443-5&partnerID=40&md5=afc02d31a0c8a01130bae89137f51b35},
  language={English},
  keywords={angiogenesis, area under the curve, artificial intelligence, breast cancer, Breast cancer prevention, cancer prevention, cancer screening, chemoprophylaxis, clinical decision making, colonoscopy, controlled study, decision making, diagnostic accuracy, diagnostic test accuracy study, digital mammography, echomammography, estrogen receptor, FAST MRI, female, fluorodeoxyglucose, genetic risk, genetic risk score, genetic screening, Genetics, genome-wide association study, glycemic index, human, lifestyle modification, liquid biopsy, mammography, nuclear magnetic resonance imaging, ovary cancer, partial mastectomy, personalized medicine, Personalized medicine, Polygenic Risk Score, predictive value, progesterone receptor, prospective study, prostate biopsy, prostate specific antigen, radiation exposure, Review, risk assessment, risk management, Risk-based screening, sensitivity and specificity, single photon emission computed tomography, systematic review, transrectal ultrasonography, triple negative breast cancer, tumor suppressor gene, Breast Neoplasms, Mass Screening},
  abstract={Purpose of Review: This paper discusses personalized risk assessment and screening, incorporating genetic and radiomic breast cancer risk information. The paper further explores opportunities to incorporate genomic information into traditional risk assessment tools, to improve the discriminatory accuracy of these tools, and integrate the information into clinical decision-making. Recent Findings: We summarize screening guidelines, strategies for identifying high risk women and discuss clinical validation studies of polygenic risk scores (PRSs). We describe the options for supplemental imaging. Both sensitivity and specificity are improving in this area, and testing is more available. Summary: Personalized screening and chemoprevention will only be possible with improved risk estimates (including breast density and PRS), meaningful, consistent shared-decision making conversations, and widespread availability of modalities for supplemental imaging. Future directions include earlier genetic testing and further validation of the PRS, particularly in non-European populations, a focus on breast density and texture in risk estimation and management and education efforts around risk communication. © 2022, The Author(s), under exclusive licence to Springer Science+Business Media, LLC, part of Springer Nature.},
  note={<p>Export Date: 15 March 2023; Cited By: 0; Correspondence Address: H.J. Pederson; Cleveland Clinic, Cleveland, 9500 Euclid Ave. A80, 44195, United States; email: Pedersh@ccf.org</p>},
  doi={10.1007/s12609-022-00443-5}
}

@article{rayyan-958927075,
  title={Systematic Review of Generative Adversarial Networks (GANs) for Medical Image Classification and Segmentation},
  year={2022},
  journal={Journal of Digital Imaging},
  issn={08971889 (ISSN)},
  volume={35},
  number={2},
  pages={137-152},
  author={Jeong, J.J. and Tariq, A. and Adejumo, T. and Trivedi, H. and Gichoya, J.W. and Banerjee, I.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85122825685&doi=10.1007%2fs10278-021-00556-w&partnerID=40&md5=5b8b30f5d2221040fef34e5af949f346},
  language={English},
  keywords={Artificial image, Computer aided diagnosis, diagnostic imaging, Generative adversarial networks, human, Humans, image analysis, Image classification, Image generation, Image generations, image processing, Image Processing, Computer-Assisted, image segmentation, Image segmentation, Images classification, Images segmentations, Informed decision, Medical image analysis, Medical image classification, Medical image segmentation, Medical imaging, meta analysis, Network architecture, Neural Networks, Computer, Preferred Reporting Items for Systematic Reviews and Meta-Analyses, procedures, review, systematic review, Systematic Review, Training dataset, Diagnostic Imaging},
  abstract={In recent years, generative adversarial networks (GANs) have gained tremendous popularity for various imaging related tasks such as artificial image generation to support AI training. GANs are especially useful for medical imaging–related tasks where training datasets are usually limited in size and heavily imbalanced against the diseased class. We present a systematic review, following the PRISMA guidelines, of recent GAN architectures used for medical image analysis to help the readers in making an informed decision before employing GANs in developing medical image classification and segmentation models. We have extracted 54 papers that highlight the capabilities and application of GANs in medical imaging from January 2015 to August 2020 and inclusion criteria for meta-analysis. Our results show four main architectures of GAN that are used for segmentation or classification in medical imaging. We provide a comprehensive overview of recent trends in the application of GANs in clinical diagnosis through medical image segmentation and classification and ultimately share experiences for task-based GAN implementations. © 2022, The Author(s) under exclusive licence to Society for Imaging Informatics in Medicine.},
  note={<p>Export Date: 15 March 2023; Cited By: 12; Correspondence Address: J.J. Jeong; Department of Biomedical Informatics, Emory School of Medicine, Atlanta, United States; email: jjeong35@asu.edu; CODEN: JDIME</p>},
  doi={10.1007/s10278-021-00556-w}
}

@article{rayyan-958927076,
  title={Trust in Human-AI Interaction: Scoping Out Models, Measures, and Methods},
  year={2022},
  journal={Conference on Human Factors in Computing Systems - Proceedings},
  issn={978-145039156-6 (ISBN)},
  author={Ueno, T. and Sawa, Y. and Kim, Y. and Urakami, J. and Oura, H. and Seaborn, K.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85129769757&doi=10.1145%2f3491101.3519772&partnerID=40&md5=a87c15d43c754a170e1e66dfbba9d3cd},
  language={English},
  publisher={Association for Computing Machinery},
  keywords={Artificial intelligence, Automation, Decision aids, Decision support systems, Human robot interaction, Intelligent robots, Interaction context, Key factors, Robots, Scoping, Scoping review, Smart vehicles, Systematic Review, Trust, Trust models, Virtual character, Humanities, Humanism, Humans},
  abstract={Trust has emerged as a key factor in people's interactions with AI-infused systems. Yet, little is known about what models of trust have been used and for what systems: robots, virtual characters, smart vehicles, decision aids, or others. Moreover, there is yet no known standard approach to measuring trust in AI. This scoping review maps out the state of affairs on trust in human-AI interaction (HAII) from the perspectives of models, measures, and methods. Findings suggest that trust is an important and multi-faceted topic of study within HAII contexts. However, most work is under-theorized and under-reported, generally not using established trust models and missing details about methods, especially Wizard of Oz. We offer several targets for systematic review work as well as a research agenda for combining the strengths and addressing the weaknesses of the current literature. © 2022 ACM.},
  note={<p>Export Date: 15 March 2023; Cited By: 0</p>},
  doi={10.1145/3491101.3519772}
}

@article{rayyan-958927078,
  title={Big Data Analytics and Healthcare Performance: A Unified Model of Competencies},
  year={2022},
  journal={2022 International Conference on Emerging Trends in Computing and Engineering Applications, ETCEA 2022 - Proceedings},
  issn={978-166547709-3 (ISBN)},
  author={Daradkeh, M. and Alkhazaleh, H.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85146948193&doi=10.1109%2fETCEA57049.2022.10009885&partnerID=40&md5=e85555e87f48c82073c613c69dbfaf23},
  language={English},
  publisher={Institute of Electrical and Electronics Engineers Inc.},
  keywords={Advanced Analytics, Big data, Big data analytic, Big Data Analytics, Classification (of information), Data analytics, Data Analytics, Decision making, Decisions makings, Health care, Healthcare, Healthcare organizations, Performance, Systematic review, Systematic Review, Thematic competency, Thematic Competency, Unified Modeling, Volume velocities},
  abstract={Due to the challenges facing healthcare data, such as volume, velocity, variety and authenticity, the healthcare industry must deploy sophisticated digital technologies to store, analyze and generate meaningful, actionable insights. Healthcare organizations can use Big Data Analytics (BDA) to analyze and classify data into useful information for decision making and performance improvement. However, implementing BDA to optimize healthcare performance can be a challenge for researchers and practitioners. In this paper, we examine how BDA is implemented and what factors influence healthcare organizations' decisions. Based on a systematic review of 25 studies, we identified five major thematic competencies: organizational, technical, analytical, cognitive, and social. A comprehensive BDA competency model was developed using these thematic categories. Using this model, we will examine the impact of BDA on healthcare performance and decision making. To close the BDA competency gap and improve the quality and competitiveness of healthcare performance, research, and practice, this model is supported by empirical evidence.  © 2022 IEEE.},
  note={<p>Export Date: 15 March 2023; Cited By: 0; Correspondence Address: M. Daradkeh; University of Dubai, College of Engineering and Information Technology, Dubai, United Arab Emirates; email: mdaradkehc@ud.ac.ae</p>},
  doi={10.1109/ETCEA57049.2022.10009885}
}

