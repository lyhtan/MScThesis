@article{rayyan-958928545,
  title={Debating big data: A literature review on realizing value from big data},
  year={2017},
  journal={Journal of Strategic Information Systems},
  issn={09638687 (ISSN)},
  volume={26},
  number={3},
  pages={191-209},
  author={Günther, W.A. and Rezazade Mehrizi, M.H. and Huysman, M. and Feldberg, F.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85030871734&doi=10.1016%2fj.jsis.2017.07.003&partnerID=40&md5=88528ba9f32d0564dde3ff9700a0e6c9},
  language={English},
  keywords={Analytics, Big data, Computer software portability, Integrated modeling, Interconnectivity, Literature review, Literature reviews, Organizational models, Portability, Societies and institutions, Stakeholder interest, Technological development, Value realization},
  abstract={Big data has been considered to be a breakthrough technological development over recent years. Notwithstanding, we have as yet limited understanding of how organizations translate its potential into actual social and economic value. We conduct an in-depth systematic review of IS literature on the topic and identify six debates central to how organizations realize value from big data, at different levels of analysis. Based on this review, we identify two socio-technical features of big data that influence value realization: portability and interconnectivity. We argue that, in practice, organizations need to continuously realign work practices, organizational models, and stakeholder interests in order to reap the benefits from big data. We synthesize the findings by means of an integrated model. © 2017 The Authors},
