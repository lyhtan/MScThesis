  language={English},
  keywords={Article, Bayes theorem, conceptual framework, decision making, drug development, Drug development, drug surveillance program, electronic health record, health care access, health care policy, health care quality, health care system, Keywords Big Data, medical research, medical technology, patient monitoring, personalized medicine, Personalized medicine, Predictive analytics, priority journal, Real world data, systematic review, Decision Making, Health Policy},
