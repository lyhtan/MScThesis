@article{rayyan-958928735,
  title={Visualization and analytics tools for infectious disease epidemiology: A systematic review},
  year={2014},
  journal={Journal of Biomedical Informatics},
  issn={15320464 (ISSN)},
  volume={51},
  pages={287-298},
  author={Carroll, L.N. and Au, A.P. and Detwiler, L.T. and Fu, T.-C. and Painter, I.S. and Abernethy, N.F.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84908031199&doi=10.1016%2fj.jbi.2014.04.006&partnerID=40&md5=37e60c8ca6268f4275cf33603145ac2e},
  language={English},
  keywords={bibliographic database, Bibliographic database, biomedical technology assessment, Cochrane Library, communicable disease control, Communicable Disease Control, Communicable Diseases, Complex networks, computer interface, confidentiality, Control and prevention, Data Sharing, Data visualization, Disease control, Disease surveillance, Diseases, Flow visualization, geographic information system, Geographic information systems, Geographic Information Systems, geographic mapping, Geographic Mapping, GIS, health survey, human, human computer interaction, Humans, infection, Infectious disease, Information analysis, Information services, Information systems, Information visualization, medical informatics, medical information, medical information system, medical record, Medline, molecular epidemiology, Molecular epidemiology, Molecular Epidemiology, Network architecture, Organizational support, Population Surveillance, procedures, Public health, public health service, publication, Review, social network, Social network analysis, Social Networking, Social networking (online), software, Software, Spatio-Temporal Analysis, spatiotemporal analysis, Systematic literature review, systematic review, Technology Assessment, Biomedical, User-Computer Interface, Visualization, Web of Science, workflow},
  abstract={Background: A myriad of new tools and algorithms have been developed to help public health professionals analyze and visualize the complex data used in infectious disease control. To better understand approaches to meet these users' information needs, we conducted a systematic literature review focused on the landscape of infectious disease visualization tools for public health professionals, with a special emphasis on geographic information systems (GIS), molecular epidemiology, and social network analysis. The objectives of this review are to: (1) identify public health user needs and preferences for infectious disease information visualization tools; (2) identify existing infectious disease information visualization tools and characterize their architecture and features; (3) identify commonalities among approaches applied to different data types; and (4) describe tool usability evaluation efforts and barriers to the adoption of such tools. Methods: We identified articles published in English from January 1, 1980 to June 30, 2013 from five bibliographic databases. Articles with a primary focus on infectious disease visualization tools, needs of public health users, or usability of information visualizations were included in the review. Results: A total of 88 articles met our inclusion criteria. Users were found to have diverse needs, preferences and uses for infectious disease visualization tools, and the existing tools are correspondingly diverse. The architecture of the tools was inconsistently described, and few tools in the review discussed the incorporation of usability studies or plans for dissemination. Many studies identified concerns regarding data sharing, confidentiality and quality. Existing tools offer a range of features and functions that allow users to explore, analyze, and visualize their data, but the tools are often for siloed applications. Commonly cited barriers to widespread adoption included lack of organizational support, access issues, and misconceptions about tool use. Discussion and conclusion: As the volume and complexity of infectious disease data increases, public health professionals must synthesize highly disparate data to facilitate communication with the public and inform decisions regarding measures to protect the public's health. Our review identified several themes: consideration of users' needs, preferences, and computer literacy; integration of tools into routine workflow; complications associated with understanding and use of visualizations; and the role of user trust and organizational support in the adoption of these tools. Interoperability also emerged as a prominent theme, highlighting challenges associated with the increasingly collaborative and interdisciplinary nature of infectious disease control and prevention. Future work should address methods for representing uncertainty and missing data to avoid misleading users as well as strategies to minimize cognitive overload. © 2014 The Authors.},
  note={<p>Export Date: 15 March 2023; Cited By: 150; CODEN: JBIOB</p>},
  doi={10.1016/j.jbi.2014.04.006}
}

@article{rayyan-958928742,
  title={The effectiveness of information technology to improve antimicrobial prescribing in hospitals: A systematic review and meta-analysis},
  year={2016},
  journal={International Journal of Medical Informatics},
  issn={13865056 (ISSN)},
  volume={92},
  pages={15-34},
  author={Baysari, M.T. and Lehnbom, E.C. and Li, L. and Hargreaves, A. and Day, R.O. and Westbrook, J.I.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84967018520&doi=10.1016%2fj.ijmedinf.2016.04.008&partnerID=40&md5=ed04a85249b7d7adfe5d0627c844fe46},
  language={English},
  keywords={Anti-Infective Agents, antibiotic agent, antibiotic resistance, antiinfective agent, Antimicrobial stewardship, Antimicrobial surveillance, Artificial intelligence, clinical decision support system, Comparative studies, comparative study, Computerized decision, Computerized decision support, computerized provider order entry, Decision support systems, Decision Support Systems, Clinical, Decision supports, drug efficacy, Drug Prescriptions, electronic medical record, Electronic medical record, electronic prescribing, Electronic Prescribing, Embedded systems, follow up, hospital, Hospitals, human, Humans, infection rate, information technology, Informed decision, length of stay, Medical computing, Medical Order Entry Systems, meta analysis, Meta analysis, Meta-analysis, mHealth, Microorganisms, mortality, Patient treatment, physician order entry system, prescription, primary medical care, priority journal, quality control, Quality control, rating scale, Review, Surveillance systems, systematic review},
  abstract={Objective: To review evidence of the effectiveness of information technology (IT) interventions to improve antimicrobial prescribing in hospitals. Method: MEDLINE (1950-March 2015), EMBASE (1947-March 2015) and PubMED (1966-March 2015) were searched for studies where an IT intervention involving any device (e.g. computer, mobile phone) was evaluated in practice. All papers were assessed for quality using a 10-point rating scale. Results: We identified 45 articles that evaluated an IT intervention to improve antimicrobial prescribing in hospitals. IT interventions took four main forms: (1) stand-alone computerized decision support systems (CDSSs), (2) decision support embedded within a hospital's electronic medical record (EMR) or computerized provider order entry (CPOE) system, (3) computerized antimicrobial approval systems (cAAS), and (4) surveillance systems (SSs). Results reported allowed us to perform meta-analyses for three outcome measures: appropriate use of antimicrobials, patient mortality and hospital length of stay (LOS). IT interventions increased appropriate use of antimicrobials (pooled RR: 1.49, 95%CI: 1.07-2.08); however no evidence of an effect was found when analysis included only studies with a quality score of five or above on the 10-point quality scale (pooled RR: 1.53, 95%CI: 0.96-2.44). There was little evidence of an effect of IT interventions on patient mortality or LOS. The range of study designs and outcome measures prevented meaningful comparisons between different IT intervention types to be made. Conclusion: IT interventions can improve the appropriateness of antimicrobial prescribing. However, high quality, systematic multi-site comparative studies are critically needed to assist organizations in making informed decisions about the most effective IT interventions. © 2016.},
  note={<p>Export Date: 15 March 2023; Cited By: 69; Correspondence Address: M.T. Baysari; Centre for Health Systems and Safety Research, Australian Institute of Health Innovation, Macquarie University, Sydney, Australia; email: m.baysari@mq.edu.au; CODEN: IJMIF</p>},
  doi={10.1016/j.ijmedinf.2016.04.008}
}

