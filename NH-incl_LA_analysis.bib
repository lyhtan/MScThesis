@article{rayyan-958927080,
  title={How well centrality measures capture student achievement in computer-supported collaborative learning? – A systematic review and meta-analysis},
  year={2022},
  journal={Educational Research Review},
  issn={1747938X (ISSN)},
  volume={35},
  author={Saqr, M. and Elmoazen, R. and Tedre, M. and López-Pernas, S. and Hirsto, L.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85123822284&doi=10.1016%2fj.edurev.2022.100437&partnerID=40&md5=c2552c0b09ed538fce2f9a0dfc2dcd04},
  language={English},
  keywords={Achievement, Centrality measures, CSCL, Educational data mining, Learning analytics, Predicting performance, Social network analysis},
  abstract={Research has shown the value of social collaboration and the benefits it brings to learners. In this study, we investigate the worth of Social Network Analysis (SNA) in translating students' interactions in computer-supported collaborative learning (CSCL) into proxy indicators of achievement. Previous research has tested the correlation between SNA centrality measures and achievement. Some results indicate a positive association, while others do not. To synthesize research efforts, investigate which measures are of value, and how strong of an association exists, this article presents a systematic review and meta-analysis of 19 studies that included 33 cohorts and 16 centrality measures. Achievement was operationalized in most of the reviewed studies as final course or task grade. All studies reported that one or more centrality measures had a positive and significant correlation with, or a potential for predicting, achievement. Every centrality measure in the reviewed sample has shown a positive correlation with achievement in at least one study. In all the reviewed studies, degree centralities correlated with achievement in terms of final course grades or other achievement measure with the highest magnitude. Eigenvector-based centralities (Eigenvector, PageRank, hub, and authority values) were also positively correlated and mostly statistically significant in all the reviewed studies. These findings emphasize the robustness and reliability of degree- and eigenvector-based centrality measures in understanding students’ interactions in relation to achievement. In contrast, betweenness and closeness centralities have shown mixed or weak correlations with achievement. Taken together, our findings support the use of centrality measures as valid proxy indicators of academic achievement and their utility for monitoring interactions in collaborative learning settings. © 2022 The Authors},
  note={<p>Export Date: 15 March 2023; Cited By: 2; Correspondence Address: M. Saqr; University of Eastern Finland, School of computing Joensuu campus, Joensuu, Yliopistokatu 2, fi, 80100, Finland; email: mohammed.saqr@uef.fi</p>},
  doi={10.1016/j.edurev.2022.100437}
}

@article{rayyan-958927083,
  title={Health Technology Assessment for In Silico Medicine: Social, Ethical and Legal Aspects},
  year={2022},
  journal={International Journal of Environmental Research and Public Health},
  issn={16617827 (ISSN)},
  volume={19},
  number={3},
  author={Leo, C.G. and Tumolo, M.R. and Sabina, S. and Colella, R. and Recchia, V. and Ponzini, G. and Fotiadis, D.I. and Bodini, A. and Mincarone, P.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85123437269&doi=10.3390%2fijerph19031510&partnerID=40&md5=83ab8734b433c10a404de90dacbaf0e9},
  language={English},
  keywords={artificial intelligence, Artificial intelligence, biomedical technology assessment, computer model, Computer modeling and simulation, computer simulation, cultural factor, Delivery of Health Care, digital twin, Digital twin technology, Ethical aspects, ethics, health care delivery, health hazard, health services, Health technology assessment, human, Humans, In silico medicine, legal aspect, Legal aspects, medical ethics, medicine, morality, Morals, numerical model, patent, patient participation, patient right, Patient Rights, privacy, Privacy, respect, Review, social aspect, Social aspects, social discrimination, systematic review, technology adoption, Technology Assessment, Biomedical, Computer Simulation},
  abstract={The application of in silico medicine is constantly growing in the prevention, diagnosis, and treatment of diseases. These technologies allow us to support medical decisions and self-management and reduce, refine, and partially replace real studies of medical technologies. In silico medicine may challenge some key principles: transparency and fairness of data usage; data privacy and protection across platforms and systems; data availability and quality; data integration and interoperability; intellectual property; data sharing; equal accessibility for persons and populations. Several social, ethical, and legal issues may consequently arise from its adoption. In this work, we provide an overview of these issues along with some practical suggestions for their assessment from a health technology assessment perspective. We performed a narrative review with a search on MEDLINE/Pubmed, ISI Web of Knowledge, Scopus, and Google Scholar. The following key aspects emerge as general reflections with an impact on the operational level: cultural resistance, level of expertise of users, degree of patient involvement, infrastructural requirements, risks for health, respect of several patients’ rights, potential discriminations for access and use of the technology, and intellectual property of innovations. Our analysis shows that several challenges still need to be debated to allow in silico medicine to express all its potential in healthcare processes. © 2022 by the authors. Licensee MDPI, Basel, Switzerland.},
  note={<p>Export Date: 15 March 2023; Cited By: 1; Correspondence Address: S. Sabina; Institute of Clinical Physiology, National Research Council, Lecce, 73100, Italy; email: sabina@ifc.cnr.it</p>},
  doi={10.3390/ijerph19031510}
}

