@ARTICLE{Moscoso2019,
author={Moscoso, A. and Silva-Rodríguez, J. and Aldrey, J.M. and Cortés, J. and Fernández-Ferreiro, A. and Gómez-Lado, N. and Ruibal, Á. and Aguiar, P. and for the Alzheimer's Disease Neuroimaging Initiative},
