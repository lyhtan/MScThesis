  volume={33},
  number={3},
  pages={456-465},
  author={López-Zambrano, J. and Torralbo, J.A.L. and Romero, C.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85111558063&doi=10.7334%2fpsicothema2021.62&partnerID=40&md5=9bd6f155c55cf9b3cedad852a41fa9b5},
