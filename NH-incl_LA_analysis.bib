  author={Dos Passos, M.G. and Amaral, L.L. and Garcia, D. and Vicente, R.B. and De Abreu, L.L.T. and De Siqueira Vieira, J.K. and De Souza Pires, M.M. and Comunello, E. and Ceretta, L.B. and Barra, C.M.C.M. and Tanaka, H. and De Carvalho, D.R. and De Oliveira, T.R. and Pires, P.D.S. and Simões, P.W. and Seroussi B. and Ohno-Machado L. and Ohno-Machado L. and Seroussi B.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85071495394&doi=10.3233%2fSHTI190349&partnerID=40&md5=d9da825d28aa3ecbca50a00adbfe5882},
  language={English},
  publisher={IOS Press},
  keywords={Artificial intelligence, Clinical, decision making, Decision Making, Decision support system (dss), Decision support systems, Decision Support Systems, diabetic retinopathy, Diabetic retinopathy, Diabetic Retinopathy, Diagnosis, expert system, Expert Systems, Eye protection, human, Humans, Imaging modality, mass screening, Mass Screening, Medical informatics, meta analysis, Meta analysis, Meta-analysis, Screening system, software, Software, Systematic Review, Visual impairment},
  abstract={Diabetic Retinopathy (DR) is one of the most common microvascular complications presenting by patients diagnosticated with diabetic diseases. Uncontrolled hyperglycemia may manifest as visual impairment and blindness. The early detection of DR is essential to minimize the risk and consequence of visual diminishing. The standard gold diagnoses tool relies on different imaging modalities and requires a judgment of expert photographers, which are not available in most of the primary care centers or remote location. In that scenario, an automate or semiautomated DR screening systems can contribute to improving the accuracy of the diagnostic. Thus, we performed a Systematic Review and Meta-Analysis to evaluate the Decision Support Systems (DSS) in diagnosing DR. The overall Diagnostic Odds Ratio was 73.15 (95%CI: 37.54-142.50), sensitivity was 97.70 (95%CI: 97.50-97.90) and specificity was 90.30 (95%CI: 90.00-90.60). Our results corroborate with the concept of usefulness of DSSs in early diagnosis, screening and preliminary evaluation of suspicious images of DR. © 2019 International Medical Informatics Association (IMIA) and IOS Press.},
  note={<p>Export Date: 15 March 2023; Cited By: 2; Correspondence Address: P.W. Simões; Engineering, Modeling and Applied Social Sciences Center (CECS), Biomedical Engineering Graduate Program (PPGEBM), Universidade Federal do ABC (UFABC), São Bernardo do Campo, Alameda da Universidade s/n, 09606-045, Brazil; email: pritsimoes@gmail.com</p>},
  doi={10.3233/SHTI190349}
}

@article{rayyan-958928197,
  title={Open-Source Big Data Analytics Architecture for Businesses},
  year={2019},
  journal={1st International Informatics and Software Engineering Conference: Innovative Technologies for Digital Transformation, IISEC 2019 - Proceedings},
  issn={978-172813992-0 (ISBN)},
  author={Gokalp, M.O. and Kayabay, K. and Zaki, M. and Kocyigit, A. and Erhan Eren, P. and Neely, A. and Varol A. and Yazici A. and Yazici M.A. and Varol C. and Aygunes G.S. and Cotur A.},
  url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85079237793&doi=10.1109%2fUBMYK48245.2019.8965572&partnerID=40&md5=467622f8c574f3922906a8c94a6f7c3d},
  language={English},
  publisher={Institute of Electrical and Electronics Engineers Inc.},
  keywords={Advanced Analytics, Big data, Big Data, Big Data Analytics, Big Data Technologies, Competitive business, Data Analytics, Data architectures, Data technologies, Open Data, Open source architecture, Open source software, Open source tools, Open systems, Reference architecture, Reference Architecture, Research domains, Systematic Review},
  abstract={Unaware of existing big data technologies, organizations fail to develop a big data capability despite its disruptive impact on today's competitive business environment. To determine the shortcomings and strengths of developing a big data architecture with open-source tools from technical and managerial perspectives, this study (1) systematically reviews the available open-source big data technologies to present a comprehensive picture, and (2) proposes an open-source architecture for businesses to take as a reference while developing big data analytics capabilities. Lastly, we discuss technical, domain-specific, and firm-specific soft challenges related to establishing a big data architecture in an organization, and how these challenges are reshaping the big data research domain. © 2019 IEEE.},
  note={<p>Export Date: 15 March 2023; Cited By: 7</p>},
